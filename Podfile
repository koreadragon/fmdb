 platform :ios, '9.0'

target 'sqlite' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
   use_frameworks!
	
   pod 'FMDB', '~> 2.7.2'
   pod 'Masonry', '~> 1.1.0'
  # Pods for sqlite

  target 'sqliteTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'sqliteUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
